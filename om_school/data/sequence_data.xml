<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <!-- Sequences for school.student -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">School Student</field>
            <field name="code">school.student</field>
            <field name="prefix">SS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_school_admission" model="ir.sequence">
            <field name="name">School Student</field>
            <field name="code">school.admission</field>
            <field name="prefix">SA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
</odoo>