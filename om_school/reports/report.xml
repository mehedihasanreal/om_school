<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_student_details" model="ir.actions.report">
        <field name="name">Student Details</field>
        <field name="model">school.student</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">om_school.report_student_details</field>
        <field name="report_file">om_school.report_student_details</field>
        <field name="binding_model_id" ref="model_school_student"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_student_details_xls" model="ir.actions.report">
        <field name="name">Student Details XLSX</field>
        <field name="model">school.student</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">om_school.report_student_details_xls</field>
        <field name="report_file">om_school.report_student_details_xls</field>
        <field name="binding_model_id" ref="model_school_student"/>
        <field name="binding_type">report</field>
    </record>

</odoo>